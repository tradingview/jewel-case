{
	"devDependencies": {
		"@jest/globals": "^29.3.1",
		"@tsconfig/esm": "^1.0.2",
		"@tsconfig/node18": "^2.0.0",
		"@tsconfig/strictest": "^2.0.0",
		"@types/node": "18.13.0",
		"@types/yargs": "^17.0.16",
		"@typescript-eslint/eslint-plugin": "^5.45.1",
		"@typescript-eslint/parser": "^5.45.1",
